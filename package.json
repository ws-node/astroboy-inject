{
  "name": "astroboy.ts",
  "version": "1.0.6-rc.11",
  "description": "基于[astroboy]的DI版本，TypeScript3.2信仰加成",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "cover": "nyc mocha src/**/*.spec.ts"
  },
  "bin": {
    "atc": "bin/atc"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ws-node/astroboy.ts.git"
  },
  "keywords": [
    "di",
    "inject",
    "astroboy"
  ],
  "author": "bigmogician",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ws-node/astroboy.ts/issues"
  },
  "homepage": "https://github.com/ws-node/astroboy.ts#readme",
  "dependencies": {
    "@bonbons/di": "1.3.3",
    "@types/koa": "^2.0.47",
    "@types/koa-router": "^7.0.35",
    "@types/node": "^10.12.10",
    "@types/uuid": "3.4.4",
    "astroboy": "0.0.37",
    "astroboy-router": "1.2.0-rc.1",
    "camelcase": "4.1.0",
    "cerialize": "0.1.18",
    "chalk": "2.4.1",
    "commander": "2.19.0",
    "decamelize": "1.2.0",
    "lodash": "^4.17.11",
    "nodemon": "1.18.8",
    "nunjucks": "3.1.6",
    "rimraf": "2.6.2",
    "tsconfig-paths": "3.7.0",
    "tslib": "^1.9.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/camelcase": "4.1.0",
    "@types/chai": "^4.1.6",
    "@types/chalk": "2.2.0",
    "@types/commander": "2.12.2",
    "@types/decamelize": "1.2.0",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "^5.2.5",
    "@types/nunjucks": "^3.1.0",
    "@types/rimraf": "^2.0.2",
    "astroboy-cli": "0.0.8",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.9",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typedoc": "^0.13.0",
    "typedoc-plugin-external-module-name": "^1.1.3",
    "typescript": "3.2.1"
  }
}